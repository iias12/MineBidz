@model MineBidz.Models.DisplayBaseViewModel

@{Domain.Concrete.Ventilation detailsModel = Newtonsoft.Json.JsonConvert.DeserializeObject<Domain.Concrete.Ventilation>(Model.DetailsInfoJson);}
<fieldset>
    <legend>@String.Format("{0}/{1}/{2}", Model.Category, Model.Subcategory, Model.FormTitle)</legend>
    <div class="createForm">
        <div>
            @Html.Label("", "Delivery: ", new { @style = "float:left" })
            @Html.Label("", Model.Contry, new { @style = "float:left; color:#000; margin-left:5px" })
            @Html.Label("", Model.Location, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
    </div>
    <div class="createForm">
        <div>
            @Html.Label("", "Tubing Type: ", new { @style = "float:left" })
            @Html.Label("", detailsModel.TubingType, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Specify if 'Other': ", new { @style = "float:left" })
            @Html.Label("", detailsModel.TubingTypeOther, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Length: ", new { @style = "float:left" })
            @Html.Label("", detailsModel.Length, new { @style = "float:left; color:#000; margin-left:5px" })
            @Html.Label("", detailsModel.LengthUnit, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Specify if 'Other': ", new { @style = "float:left" })
            @Html.Label("", detailsModel.LengthUnitOther, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
    </div>
    <div class="createForm">
        <div>
            @Html.Label("", "Diameter: ", new { @style = "float:left" })
            @Html.Label("", detailsModel.Diameter, new { @style = "float:left; color:#000; margin-left:5px" })
            @Html.Label("", detailsModel.DiameterUnit, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Specify if 'Other': ", new { @style = "float:left" })
            @Html.Label("", detailsModel.DiameterUnitOther, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Quantity of Couplings (ea): ", new { @style = "float:left" })
            @Html.Label("", detailsModel.CouplingsQuantity, new { @style = "float:left; color:#000; margin-left:5px" })
            @Html.Label("", detailsModel.CouplingsQuantityUnit, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Specify if 'Other': ", new { @style = "float:left" })
            @Html.Label("", detailsModel.CouplingsQuantityUnitOther, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
    </div>
    <div class="createForm">
        <div>
            @Html.Label("", "Quantity of Fans (ea): ", new { @style = "float:left" })
            @Html.Label("", detailsModel.FansQuantity, new { @style = "float:left; color:#000; margin-left:5px" })
            @Html.Label("", detailsModel.FansQuantityUnit, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Specify if 'Other': ", new { @style = "float:left" })
            @Html.Label("", detailsModel.FansQuantityUnitOther, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "SCFM: ", new { @style = "float:left" })
            @Html.Label("", detailsModel.Scfm, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Type: ", new { @style = "float:left" })
            @Html.Label("", detailsModel.Type, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Specify if 'Other': ", new { @style = "float:left" })
            @Html.Label("", detailsModel.TypeOther, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
    </div>
    <div class="createForm">
        <div>
            @Html.Label("", "Silencer: ", new { @style = "float:left" })
            @Html.Label("", detailsModel.Silencer, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Specify if 'Other': ", new { @style = "float:left" })
            @Html.Label("", detailsModel.SilencerOther, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Accessories: ", new { @style = "float:left" })
            @Html.Label("", detailsModel.Accessories, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Specify if 'Other': ", new { @style = "float:left" })
            @Html.Label("", detailsModel.AccessoriesOther, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
    </div>
    <div class="createForm">
        <div>
            @Html.Label("", "Damper: ", new { @style = "float:left" })
            @Html.Label("", detailsModel.Damper, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Specify if 'Other': ", new { @style = "float:left" })
            @Html.Label("", detailsModel.DamperOther, new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Heaters (# of btu): ", new { @style = "float:left" })
            @Html.Label("", detailsModel.HeatersBtuNumber, new { @style = "float:left; color:#000; margin-left:5px" })
            @Html.Label("", detailsModel.HeatersBtuNumberYesNo.ToString(), new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
        <div>
            @Html.Label("", "Air Conditioning (tons): ", new { @style = "float:left" })
            @Html.Label("", detailsModel.AirConditioningTons, new { @style = "float:left; color:#000; margin-left:5px" })
            @Html.Label("", detailsModel.AirConditioningTonsYesNo.ToString(), new { @style = "float:left; color:#000; margin-left:5px" })
        </div>
    </div>
    @{Html.RenderPartial("PartialDisplayMotorIncluded", detailsModel.MotorIncluded);}
    @if (detailsModel.MotorIncluded && detailsModel.Motor != null) { Html.RenderPartial("PartialDisplayMotor", detailsModel.Motor); }
    @{Html.RenderPartial("PartialDisplayCommon");}
</fieldset>

@{Html.RenderPartial("PartialDisplayPlaceBid");}
